{
    "name": "klipper/functional-test-bundle",
    "type": "symfony-bundle",
    "description": "Functional Test bundle of Klipper",
    "license": "MIT",
    "authors": [
        {
            "name": "Fran√ßois Pluchino",
            "email": "francois.pluchino@klipper.dev"
        }
    ],
    "require": {
        "php": "^7.4.0",
        "symfony/framework-bundle": "^4.4.0",
        "doctrine/data-fixtures": "^1.4.0",
        "doctrine/doctrine-fixtures-bundle": "^3.3.0",
        "doctrine/doctrine-bundle": "^2.0.0",
        "doctrine/orm": "^2.7.0",
        "klipper/api-bundle": "^2.0.0",
        "klipper/data-loader": "^2.0.0",
        "klipper/doctrine-extra": "^2.0.0",
        "klipper/resource-bundle": "^2.0.0",
        "ramsey/uuid": "^3.7.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "stof/doctrine-extensions-bundle": "^1.4.0",
        "symfony/console": "^4.4.0",
        "symfony/doctrine-bridge": "^4.4.0",
        "symfony/property-access": "^4.4.0",
        "symfony/process": "^4.4.0",
        "symfony/validator": "^4.4.0"
    },
    "autoload": {
        "psr-4": { "Klipper\\Bundle\\FunctionalTestBundle\\": "" },
        "exclude-from-classmap": [
            "**/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
